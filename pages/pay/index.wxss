.address .address_info {
  padding: 10rpx;
  color: #333333;
  font-size: 32rpx;
}
view.cart view.cart_title {
  color: var(--themeColor);
  border-top: 1rpx solid currentColor;
  border-bottom: 1rpx solid currentColor;
  font-size: 36rpx;
  padding: 20rpx;
}
view.cart view.cart_list view.cart_item {
  height: 156rpx;
  display: flex;
  border-bottom: 1rpx solid #cccccc;
  padding: 6rpx;
}
view.cart view.cart_list view.cart_item view.cart_img_wrap {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
view.cart view.cart_list view.cart_item view.cart_img_wrap image {
  width: 60%;
}
view.cart view.cart_list view.cart_item view.cart_info_wrap {
  flex: 4;
}
view.cart view.cart_list view.cart_item view.cart_info_wrap view.goods_name {
  display: -webkit-box;
  overflow: hidden;
  white-space: normal !important;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
view.cart view.cart_list view.cart_item view.cart_info_wrap view.cart_tool {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx;
}
view.cart view.cart_list view.cart_item view.cart_info_wrap view.cart_tool view.goods_price {
  color: var(--themeColor);
  font-size: 32rpx;
}
view.cart view.cart_list view.cart_item view.cart_info_wrap view.cart_tool view.goods_nums_wrap {
  display: flex;
  justify-content: space-around;
}
view.cart view.cart_list view.cart_item view.cart_info_wrap view.cart_tool view.goods_nums_wrap view.goods_num {
  margin: 0 20rpx;
}
view.btm_cart {
  width: 100%;
  height: 90rpx;
  background-color: #f3f3f3;
  position: fixed;
  left: 0;
  bottom: 0;
  border-top: 1rpx solid #000000;
  display: flex;
}
view.btm_cart view.total_price_wrap {
  flex: 6;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  justify-content: space-around;
  padding-right: 10rpx;
}
view.btm_cart view.total_price_wrap view.total_price text {
  font-size: 32rpx;
  color: var(--themeColor);
  font-weight: 600;
}
view.btm_cart view.cart_order {
  flex: 4;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: var(--themeColor);
  color: #f3f3f3;
  font-weight: 600;
}

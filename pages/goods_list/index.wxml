<!--pages/goods_list/index.wxml-->
<SearchInput></SearchInput>
<ShopTabs titleList="{{titleList}}"
          currentIndex="{{currentIndex}}"
          bind:changeIndex="handleChangeIndex"
>
    <view class="list_content" wx:if="{{currentIndex===0}}">
        <navigator class="goods_item"
            wx:for="{{goodsList}}"
                   wx:key="goods_id"
                   url="/pages/goods_detail/index?goods_id={{item.goods_id}}"
        >
            <view class="img_container">
                <image mode="widthFix" src="{{item.goods_small_logo||'data:image/jpeg;base64,'}}"></image>
            </view>
            <view class="goods_info">
                <view class="goods_name">{{item.goods_name}}</view>
                <view class="goods_price">￥{{item.goods_price}}</view>
            </view>
        </navigator>
        <view class="list_no_more" wx:if="{{isNoMore}}">
            ---没有数据了---
        </view>
    </view>
    <view class="list_content" wx:elif="{{currentIndex===1}}">1</view>
    <view class="list_content" wx:else>2</view>
</ShopTabs>
